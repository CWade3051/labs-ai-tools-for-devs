{
  "icon": "docker.svg",
  "vm": {
    "composefile": "docker-compose.yaml",
    "exposes": {
      "socket": "backend.sock"
    }
  },
  "ui": {
    "dashboard-tab": {
      "title": "MCP Catalog",
      "src": "index.html",
      "root": "ui"
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/darwin/host-binary"
          }
        ],
        "linux": [
          {
            "path": "/linux/host-binary"
          }
        ],
        "windows": [
          {
            "path": "/windows/host-binary.exe"
          }
        ]
      }
    ]
  }
}
